// Jenkinsfile (Declarative Pipeline)
pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'Starting Build..'
            }
        }
        stage('test') {
            steps {
                echo 'Starting Test..'
            }
        }
        stage('deploy') {
            steps { 
                echo 'Starting Deployment..'
            }
        }
    }
}
